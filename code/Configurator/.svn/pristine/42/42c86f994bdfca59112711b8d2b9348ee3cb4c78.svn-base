                <Setting Name="Over Range Threshold"   RangeMin="0" RangeMax="100.0" Units="Percent" Variable="kAPI1R" /> 
                <Setting Name="Signal Filtering"          RangeMin="0" RangeMax="99"    Units="Percent" Variable="kAPI1F" />
                <Setting Name="Input Signal Type"                                       Units="Boolean" Variable="gAPI1IT"  OptionList="Input Type" />
            </Setting>
            <Setting Name="Position Feedback Input 02 ( PV 02 )"> 
                <Setting Name="Over Range Threshold"   RangeMin="0" RangeMax="100.0" Units="Percent" Variable="kAPI2R" />
                <Setting Name="Signal Filtering"          RangeMin="0" RangeMax="99"    Units="Percent" Variable="kAPI2F" />
                <Setting Name="Input Signal Type"                                       Units="Boolean" Variable="gAPI2IT"  OptionList="Input Type" />
            </Setting>
            <Setting Name="Analog Position Demand"> 
                <Setting Name="Minimum Current"           RangeMin="0" RangeMax="25.0"  Units="mA" Variable="kAI_APDMIN" />
                <Setting Name="Maximum Current"           RangeMin="0" RangeMax="25.0"  Units="mA" Variable="kAI_APDMAX" />
                <Setting Name="Over Range Threshold"   RangeMin="0" RangeMax="100.0" Units="Percent" Variable="kAPDAIORT" /> 
                <Setting Name="Signal Filtering"          RangeMin="0" RangeMax="99"    Units="Percent" Variable="kAPDFIL" /> 
                <!--
                <Setting Name="Over Range Response"                                  Units="Boolean" Variable="gAPDFR"      OptionList="Ain Assignment" /> 
                -->
            </Setting>
        </Setting> 
         <Setting Name="Analog Outputs" HelpName="HIDX_INOUT_AO">
           <Setting Name="Actuator">
               <Setting Name="Current Range (Jumper Select)"             Units="Option"   Variable="nActCurSel"             OptionList="Actuator Current Range"/>
               <Setting Name="Minimum Current"                           RangeMin="-10.0" RangeMax="10.0"  Units="mA"       Variable="kAO1MIN"/>
               <Setting Name="Maximum Current"                           RangeMin="-10.0" RangeMax="10.0"  Units="mA"       Variable="kAO1MAX"/>
               <Setting Name="Null Current"                              RangeMin="-10.0" RangeMax="10.0"  Units="mA"       Variable="kAONULL1"/>
               <Setting Name="Shutdown Current"                          RangeMin="-10.0" RangeMax="10.0"  Units="mA"       Variable="kSDCurrent"/>
               <Setting Name="Readback Alarm Threshold"                  RangeMin="1"     RangeMax="30"    Units="Percent"  Variable="kAlmRD"/>
               <Setting Name="Actuator 01 Enable"                                    Units="Boolean"  Variable="bAO1Enable" />
               <Setting Name="Actuator 02 Enable"                                    Units="Boolean"  Variable="bAO2Enable" />
               <Setting Name="Dither Current Amplitude"    RangeMin="0"   RangeMax="10.0"   Units="Percent" Variable="kPD" /> 
               <Setting Name="Dither Frequency "           RangeMin="0"   RangeMax="100.0"  Units="Hz"      Variable="kPF" /> 
