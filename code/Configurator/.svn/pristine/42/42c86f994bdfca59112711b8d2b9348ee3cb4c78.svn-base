               <Setting Name="Double Actuator Current When AO1 or AO2 Open"  Units="Boolean"  Variable="bDoubleFlag"/>
               -->
           </Setting>
           <Setting Name="Position Monitor"> 
               <Setting Name="Minimum Current"    RangeMin="0"   RangeMax="25.0"  Units="mA"      Variable="kAO3MIN" />
               <Setting Name="Maximum Current"    RangeMin="0"   RangeMax="25.0"  Units="mA"      Variable="kAO3MAX" />
               <Setting Name="Readback Threshold" RangeMin="1.0" RangeMax="100.0" Units="Percent" Variable="kAO3RB" />
