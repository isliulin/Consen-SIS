               <Setting Name="Action" Units="Boolean"  Variable="kDI1S" OptionList="Reset Assignment" />
            </Setting>
            <Setting Name="External ShutDown">
               <Setting Name="Action" Units="Boolean"  Variable="kDI2S" OptionList="Shutdown Assignment" />
            </Setting>
        </Setting>
        <Setting Name="Digital Outputs" HelpName="HIDX_INOUT_DO">
            <Setting Name="Alarm Relay Output">
               <Setting Name="Alarm Output Latch" Units="Boolean"  Variable="cD01S" OptionList="Latch Assignment" /> 
            </Setting>
        </Setting>
    </Setting>
    <Setting Name="Control" HelpName="HIDX_CONTROL">
        <Setting Name="Control Period" HelpName="HIDX_CONTROL_PERIOD">
            <Setting Name="Control Period"   RangeMin="10"   RangeMax="65535"    Units="ms" Variable="nCtlP"  />
        </Setting> 
        <Setting Name="PV( Analog Position Input )" HelpName="HIDX_CONTROL_PV">
            <Setting Name="Select Algorithm"                                                  Units="Option" Variable="nPVSel"      OptionList="PV Type" />
            <Setting Name="Deviation Alarm Setpoint"            RangeMin="0" RangeMax="100.0" Units="Percent" Variable="kDEVARMSP"  />
            <Setting Name="PV Open/Over Range Shutdown Select"                                Units="Option" Variable="nPVORFault"  OptionList="PV Fault Action"/>
        </Setting>  
        <Setting Name="SP( Position Demand )" HelpName="HIDX_CONTROL_SP">
            <Setting Name="Select Algorithm"                                                     Units="Option"  Variable="nSPSel"  OptionList="SP Type" />
            <Setting Name="Latch When Position Demand Fault"                                     Units="Boolean" Variable="gAI_SPL"  />
            <Setting Name="Position Demand Fault Response"                                       Units="Boolean" Variable="gAI_SPER" OptionList="Ain Assignment"/>
            <Setting Name="Redundant Tracking Error Threshold"   RangeMin="1.0" RangeMax="100.0" Units="Percent" Variable="kAI_SPET"  />
            <Setting Name="Redundant Tracking Error Delay Time"                                  Units="Seconds" Variable="kAI_SPED" />  
        </Setting> 
            <Setting Name="Error = ABS(SP-PV)" HelpName="HIDX_CONTROL_ERROR">
            <Setting Name="Error Threshold" RangeMin="0"   RangeMax="100.0"   Units="Percent" Variable="nDEV_TS" /> 
            <Setting Name="Error Delay Time"               Units="Seconds" Variable="kDEVDelayT" />  
            <Setting Name="Error Response"    Units="Boolean" Variable="gSPPVFR" OptionList="SPPV Response" />
        </Setting> 
        <Setting Name="Tuning" HelpName="HIDX_TUNING">
            <Setting Name="Error Threshold"        RangeMin="0" RangeMax="100.0" Units="Percent" Variable="kPIDErrT" />
            <Setting Name="Tuning1( When ABS(SP-PV) greater than Error Threshold )">
                <Setting Name="Proportional Band"  RangeMin="0" RangeMax="1000.0" Units="Percent"    Variable="kPID1PB" />
                <Setting Name="Reset"              RangeMin="0" RangeMax="400.0"  Units="Sec/Repeat" Variable="kPID1RES" />
                <Setting Name="Rate"               RangeMin="0" RangeMax="10.0"   Units="Seconds"    Variable="kPID1RATE" />
            </Setting>
            <Setting Name="Tuning2( When ABS(SP-PV) less than Error Threshold )">   
                <Setting Name="Proportional Band"  RangeMin="0" RangeMax="1000.0" Units="Percent"    Variable="kPID2PB" />
                <Setting Name="Reset"              RangeMin="0" RangeMax="400.0"  Units="Sec/Repeat" Variable="kPID2RES" />
                <Setting Name="Rate"               RangeMin="0" RangeMax="10.0"   Units="Seconds"    Variable="kPID2RATE" />
            </Setting>
            <!--
            <Setting Name="PID Max Rate"           RangeMin="0" RangeMax="100.0"  Units="Value" Variable="kPIDMaxRate" />-->
            <Setting Name="PID Controller Output Range" HelpName="HIDX_PID_CONTROLLER_OUTPUT">  
                <Setting Name="Minimum Output"          RangeMin="0"    RangeMax="10.0"  Units="Percent" Variable="kPIDMINP" />
                <Setting Name="Maximum Output"          RangeMin="90.0" RangeMax="100.0" Units="Percent" Variable="kPIDMAXP" />
            </Setting>
        </Setting>
        <!--
        <Setting Name="Driver" HelpName="HIDX_DRIVE">
            <Setting Name="Dither Current Amplitude"    RangeMin="0"   RangeMax="10.0"   Units="Percent" Variable="kPD" /> 
            <Setting Name="Dither Frequency "           RangeMin="0"   RangeMax="100.0"  Units="Hz"      Variable="kPF" />  
        </Setting>
        -->
    </Setting> 
    <Setting Name="MAC Address"                                                 Units="String"  Variable="strMAC" />
    </Settings>   
